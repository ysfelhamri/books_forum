{
    "builds": [{
        "src": "books_forum/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/public/stati/$1"
        },
        {
            "src": "/(.*)",
            "dest": "books_forum/wsgi.py"
        }
    ]
}